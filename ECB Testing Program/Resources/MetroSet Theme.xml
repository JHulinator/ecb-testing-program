    <CloseHoverBackColor>#c81e1e</CloseHoverBackColor>
    <CloseHoverForeColor>#1E1E1E</CloseHoverForeColor>
    <CloseNormalForeColor>#808080</CloseNormalForeColor>
    <MaximizeHoverBackColor>#EEEEEE</MaximizeHoverBackColor>
